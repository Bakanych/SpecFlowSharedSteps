<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net45</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Product>SpecFlow Shared Steps</Product>
    <Description>This package contains SpecFlow steps definitions which allow you to reuse scenario steps</Description>
    <PackageId>SpecFlow.SharedSteps</PackageId>
    <Authors>Alexander Bakanov</Authors>
    <Company />
    <Copyright>Copyright ©  2018</Copyright>
    <PackageProjectUrl>https://github.com/Bakanych/SpecFlowSharedSteps</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/Bakanych/SpecFlowSharedSteps/master/shared-steps-nuget-icon.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/Bakanych/SpecFlowSharedSteps</RepositoryUrl>
    <PackageTags>specflow bdd</PackageTags>
    <Version>2.0.0</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Castle.Core" Version="3.2.0" />
    <PackageReference Include="SpecFlow" Version="2.2.0" />
    <Content Include="App.config.transform" Pack="true" />
  </ItemGroup>
</Project>